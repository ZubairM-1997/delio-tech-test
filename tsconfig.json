{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "target": "es2018",
      "esModuleInterop": true
    },
    "include": ["src/**/*"]
}
  